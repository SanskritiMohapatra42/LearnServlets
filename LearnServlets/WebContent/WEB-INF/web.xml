<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>LearnServlets</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>ServletInterface</servlet-name>
  	<servlet-class>servlets.ServletInterface</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ServletInterface</servlet-name>
  	<url-pattern>/interface</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>ServletGeneric</servlet-name>
  	<servlet-class>servlets.ServletGeneric</servlet-class>
  </servlet>
  <servlet-mapping>
  		<servlet-name>ServletGeneric</servlet-name>
  		<url-pattern>/generic</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>ServletHttp</servlet-name>
  	<servlet-class>servlets.ServletHttp</servlet-class>
  </servlet>
  <servlet-mapping>
  		<servlet-name>ServletHttp</servlet-name>
  		<url-pattern>/httpservlet</url-pattern>  
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>AjaxController</servlet-name>
  	<servlet-class>servlets.AjaxController</servlet-class>
  </servlet>
  <servlet-mapping>
  		<servlet-name>AjaxController</servlet-name>
  		<url-pattern>/ajax</url-pattern>
  
  </servlet-mapping>
  
  
  
  
</web-app>